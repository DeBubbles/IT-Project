<head>
  <meta charset="UTF-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <link
    rel="stylesheet"
    href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css"
  />
  <link rel="stylesheet" href="../css/reset.css" />
  <link rel="stylesheet" href="../css/style.css" />

  <link rel="preconnect" href="https://fonts.googleapis.com" />
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
  <link
    href="https://fonts.googleapis.com/css2?family=Nunito&family=Roboto&display=swap"
    rel="stylesheet"
  />
  <title>Home</title>
</head>
<body>
  <header>
    <figure>
      <img src="../img/Logo.png" alt="Logo" />
    </figure>
    <nav>
      <ul>
        <li><a href="../mtg">Home</a></li>
        <li><a href="decks">Decks</a></li>
        <li><a href="draw" id="active">DrawTest</a></li>
      </ul>
    </nav>
  </header>
  <main id="draw">
    <form>
      <select id="collections">
        <% for (let i = 0;i < collections.length; i++ ){%>
        <option value="<%= collections[i].name %>">
          Deck: <%= collections[i].name %>
        </option>
        <%}%>
      </select>
    </form>
    <section>
      <section>
        <figure>
          <img src="../img/back-of-card.jpg" alt="" />
        </figure>
      </section>
      <input type="submit" value="Draw" />
      <input type="submit" value="reset" />
      <section>
        <figure>
          <img src="../img/back-of-card.jpg" alt="" />
        </figure>
      </section>
    </section>
  </main>
  <footer>
    <p>IT-Project 2021 - 2022</p>
    <p>&copy; Orzhov Cartel</p>
  </footer>
</body>
